{% extends "VersionControlGitControlBundle::admincontent.layout.html.twig" %}



            
{% block header %}
    
    
    <h1 class="page-header">
          Edit Issue #{{issue.id}}: <small>{{issue.title}}</small>
    </h1>
    <ol class="breadcrumb">
        <li>
            <a href="{{ path('home')}}"><i class="fa fa-product-hunt"></i> Projects</a>
        </li>
        <li >
            <i class="fa fa-bug"></i> <a href="{{path('issues', { 'id': project.id }) }}" class="non-ajax">Issues</a>
        </li>
        <li class="active">
                <i class="fa fa-file"></i> Edit Issue
            </li>
    </ol>
{% endblock %}



{% block content -%}
 
    <div class="box box-primary">
        <div class="box-header with-border">
            <h3 class="box-title">
                <i class="fa fa-bug fa-fw"></i> Edit Issue
            </h3>
        </div>
        <div class="box-body">
 
                {{ form(edit_form) }}
                
        </div>
    </div>
{% endblock %}