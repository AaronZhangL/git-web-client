<!DOCTYPE html>
<html lang="{{ app.request.get('_locale') }}" > 

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="An online versioning control">
    <meta name="author" content="Paul Schweppe">

  <title>{% block title %}Commit{% endblock %}</title>
  
    {% block head %}
        <link rel="icon" sizes="16x16" href="{{ asset('favicon.ico') }}" />
         <!-- Bootstrap Core CSS -->

        <link href="{{ asset('bundles/VersionContol/css/bootstrap.min.css') }}" rel="stylesheet">

        <!-- Custom CSS -->
        <link href="{{ asset('bundles/VersionContol/css/sb-admin.css') }}" rel="stylesheet">

        <!-- Custom Fonts -->
        <link href="{{ asset('bundles/VersionContol/font-awesome/css/font-awesome.min.css') }}" rel="stylesheet" type="text/css">
        
        <link href="{{ asset('bundles/VersionContol/css/custom.css') }}" rel="stylesheet" type="text/css">

        <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
        <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
        <!--[if lt IE 9]>
            <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
            <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
        <![endif]-->
	
    {% endblock %}    
</head>
<body>
        
        {% block body %}
            <div id="wrapper-home">
             <!-- Navigation -->
            <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
                <!-- Brand and toggle get grouped for better mobile display -->
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand" href="index.html">Version Control</a>
                </div>
            </nav>

            <div id="page-wrapper">

                <div class="container-fluid">

                    <!-- Page Heading -->
                    <div class="row">
                        <div class="col-lg-12">
                           {% block hwi_oauth_content %}
                           {% endblock hwi_oauth_content %}
                            
                        </div>               
                    </div>
                    <!-- /.row -->

                </div>
                <!-- /.container-fluid -->

            </div>
            <!-- /#page-wrapper -->

        </div>
        <!-- /#wrapper -->
            
        {% endblock %}

        {% block footer %}
             <!-- jQuery -->
            <script src="{{ asset('bundles/VersionContol/js/jquery.js') }}"></script>

            <!-- Bootstrap Core JavaScript -->
            <script src="{{ asset('bundles/VersionContol/js/bootstrap.min.js') }}"></script>

        {% endblock %} 
            
        {% block footerJS %}
        {% endblock %} 

</body>
</html>