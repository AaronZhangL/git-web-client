<!DOCTYPE html>
<html lang="{{ app.request.get('_locale') }}">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="description" content="An Online Versioning Control System for Git">
    <meta name="author" content="Paul Schweppe">

    <title>{% block title %}Commit{% endblock %}</title>
    {% block head %}
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    
    <link rel="apple-touch-startup-image" href="/startup-image.png">
    <!-- iPhone SPLASHSCREEN-->
    <link href="apple-touch-startup-image-320x460.png" media="(device-width: 320px)" rel="apple-touch-startup-image">
    <!-- iPhone (Retina) SPLASHSCREEN-->
    <link href="apple-touch-startup-image-640x920.png" media="(device-width: 320px) and (-webkit-device-pixel-ratio: 2)" rel="apple-touch-startup-image">
    <!-- iPad (portrait) SPLASHSCREEN-->
    <link href="apple-touch-startup-image-768x1004.png" media="(device-width: 768px) and (orientation: portrait)" rel="apple-touch-startup-image">
    <!-- iPad (landscape) SPLASHSCREEN-->
    <link href="apple-touch-startup-image-748x1024.png" media="(device-width: 768px) and (orientation: landscape)" rel="apple-touch-startup-image">
    <!-- iPad (Retina, portrait) SPLASHSCREEN-->
    <link href="apple-touch-startup-image-1536x2008.png" media="(device-width: 1536px) and (orientation: portrait) and (-webkit-device-pixel-ratio: 2)" rel="apple-touch-startup-image">
    <!-- iPad (Retina, landscape) SPLASHSCREEN-->
    <link href="apple-touch-startup-image-2048x1496.png" media="(device-width: 1536px)  and (orientation: landscape) and (-webkit-device-pixel-ratio: 2)" rel="apple-touch-startup-image">

    <meta name="apple-mobile-web-app-title" content="Git Web Client">
    <meta name="apple-mobile-web-app-capable" content="yes">
    
    <!-- Bootstrap 3.3.5 -->
    <link rel="stylesheet" href="{{ asset('bundles/versioncontrolgitcontrol/bootstrap/css/bootstrap.min.css') }}" >
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <!-- Theme style -->
    {% block includeCSSBefore %}
    {% endblock %}
    <link rel="stylesheet" href="{{ asset('bundles/versioncontrolgitcontrol/theme/css/AdminLTE.min.css') }}" >
    <link rel="stylesheet" href="{{ asset('bundles/versioncontrolgitcontrol/theme/css/skins/skin-green.min.css') }}" >
    
    <link href="{{ asset('bundles/versioncontrolgitcontrol/theme/css/pick-a-color-1.2.3.min.css') }}" rel="stylesheet" type="text/css">
    <link href="{{ asset('bundles/versioncontrolgitcontrol/js/select2/v3.5/select2.css') }}" rel="stylesheet" type="text/css">
    <link href="{{ asset('bundles/versioncontrolgitcontrol/js/select2/v3.5/select2-bootstrap.css') }}" rel="stylesheet" type="text/css">
    
    <link href="{{ asset('bundles/versioncontrolgitcontrol/theme/plugins/jquery-ui/jquery-ui.min.css') }}" rel="stylesheet" type="text/css">

    <link href="{{ asset('bundles/versioncontrolgitcontrol/theme/css/custom.css') }}" rel="stylesheet" type="text/css">
    
    <link href="{{ asset('bundles/versioncontrolgitcontrol/theme/plugins/prism/prism.css') }}" rel="stylesheet" type="text/css">
    <link href="{{ asset('bundles/versioncontrolgitcontrol/theme/plugins/pace/pace.min.css') }}" rel="stylesheet" type="text/css">
    
    <link href="{{ asset('bundles/versioncontrolgitcontrol/theme/plugins/loadmask/jquery.loadmask.spin.css') }}" rel="stylesheet" type="text/css">
    
    {% block includeCSSAfter %}
    {% endblock %}
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    {% endblock %}    
  </head>
 
  <body class="hold-transition skin-green sidebar-mini">
    {% set route = app.request.get('_route') %}
    {% set params = app.request.get('_route_params') | merge(app.request.query.all) %}
    {% block body %}
            <div class="wrapper">

        <!-- Main Header -->
        <header class="main-header">

          <!-- Logo -->
          <a class="logo" href="{{ path('home')}}">
              <!-- mini logo for sidebar mini 50x50 pixels -->
              <span class="logo-mini"><b>V</b>C</span>
              <!-- logo for regular state and mobile devices -->
              <span class="logo-lg"><b>Version</b> Control</span>
          </a>

          <!-- Header Navbar -->
          <nav class="navbar navbar-static-top" role="navigation">
            <!-- Sidebar toggle button-->
            <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
              <span class="sr-only">Toggle navigation</span>
            </a>
            
            <!-- Navbar Left Menu -->
            
            {% block topnavleft %}
            {% endblock %}
          
            <!-- /.Navbar Left Menu -->
            <!-- Navbar Right Menu -->
            <div class="navbar-custom-menu">
              <ul class="nav navbar-nav">
                  {% block topnav %}
                  {% endblock %}
                  
                <!-- User Account Menu -->
                <li class="dropdown user user-menu">
                  <!-- Menu Toggle Button -->
                  <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                    <!-- The user image in the navbar-->
                    <img src="{{ asset('bundles/versioncontrolgitcontrol/images/default_user_icon.jpg') }}" class="user-image" alt="User Image">
                    <!-- hidden-xs hides the username on small devices so only the image appears. -->
                    <span class="hidden-xs">{{ app.user.name }}</span>
                  </a>
                  <ul class="dropdown-menu">
                    <!-- The user image in the menu -->
                    <li class="user-header">
                      <img src="{{ asset('bundles/versioncontrolgitcontrol/images/default_user_icon.jpg') }}" class="img-circle" alt="User Image">
                      <p>
                        {{ app.user.name }} - Web Developer
                        <small>Member since Nov. 2012</small>
                      </p>
                    </li>
                    <!-- Menu Body -->
                    <li class="user-body">
                      {% if (is_granted('ROLE_ADMIN')) %} 
                          <div class="col-xs-4 text-center">
                             <a href="{{path('usermanagement')}}">Admin</a>
                          </div>
                          <div class="col-xs-4 text-center">
                             <a href="{{path('home')}}">Projects</a>
                          </div>
                          <div class="col-xs-4 text-center">
                             <a href="#">Friends</a>
                          </div>
                          {%else%}
                          <div class="col-xs-6 text-center">
                             <a href="{{path('home')}}">Projects</a>
                          </div>
                          <div class="col-xs-6 text-center">
                             <a href="#">Friends</a>
                          </div>    
                      {% endif %}
                    </li>
                    <!-- Menu Footer-->
                    <li class="user-footer">
                      <div class="pull-left">
                        <a href="{{path('fos_user_profile_show')}}" class="btn btn-default btn-flat">Profile</a>
                      </div>
                      <div class="pull-right">
                        <a href="{{ path('fos_user_security_logout') }}" class="btn btn-default btn-flat">Sign out</a>
                      </div>
                    </li>
                  </ul>
                </li>
                <!-- Control Sidebar Toggle Button -->
                <li>
                  <a href="#" data-toggle="control-sidebar"><i class="fa fa-gears"></i></a>
                </li>
              </ul>
            </div>
          </nav>
        </header>
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">

          <!-- sidebar: style can be found in sidebar.less -->
          <section class="sidebar">

            <!-- Sidebar user panel (optional) -->
            <div class="user-panel">
              <div class="pull-left image">
                <img src="{{ asset('bundles/versioncontrolgitcontrol/images/default_user_icon.jpg') }}" class="img-circle" alt="User Image">
              </div>
              <div class="pull-left info">
                <p>{{ app.user.name }}</p>
                <!-- Status -->
                <a href="#"><i class="fa fa-circle text-success"></i> Online</a>
              </div>
            </div>
            
            {% block sidemenu %}
            <!-- Sidebar Menu -->
                <ul class="sidebar-menu">
                  <li class="header">MAIN NAVIGATION</li>
                  <!-- Optionally, you can add icons to the links -->
                  <li class="active">
                        <a href="{{ path('home') }}"><i class="fa fa-fw fa-product-hunt"></i><span>My Projects</span></a>
                  </li>
                  
                </ul><!-- /.sidebar-menu -->
            {% endblock %}
          </section>
          <!-- /.sidebar -->
        </aside>

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            {% block topcontent %}{% endblock %} 
            
            <!-- Alert Flash Messages -->
            <div class="alert-header">

                {% for flashMessage in app.session.flashbag.get('notice') %}
                    <div class="alert alert-success alert-dismissible">
                        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                        <h4><i class="icon fa fa-info"></i> Notice</h4>
                        {{ flashMessage|nl2br }}
                    </div>
                {% endfor %}
                {% for flashMessage in app.session.flashbag.get('success') %}
                    <div class="alert alert-success alert-dismissible">
                        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                        <h4><i class="icon fa fa-check"></i> Success</h4>
                        {{ flashMessage|nl2br }}
                    </div>
                {% endfor %}
                {% for flashMessage in app.session.flashbag.get('error') %}
                    <div class="alert alert-danger alert-dismissible">
                        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                        <h4><i class="icon fa fa-ban"></i> Error</h4> 
                        {{ flashMessage|nl2br }}
                    </div>
                {% endfor %}
                {% for flashMessage in app.session.flashbag.get('warning') %}
                    <div class="alert alert-warning alert-dismissible">
                        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                        <h4><i class="icon fa fa-warning"></i> Warning</h4>  
                        {{ flashMessage|nl2br }}
                    </div>
                {% endfor %}
                {% for flashMessage in app.session.flashbag.get('info') %}
                    <div class="alert alert-info alert-dismissible">
                        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                        <h4><i class="icon fa fa-info"></i> Info</h4> 
                        {{ flashMessage|nl2br }}
                    </div>
                {% endfor %}
   
                {% block otherAlerts %}
                {% endblock %}
            </div>

                    
          <!-- Content Header (Page header) -->
          <section class="content-header">
              {% block header %}
                <h1>
                  Page Header
                  <small>Optional description</small>
                </h1>
                <ol class="breadcrumb">
                  <li><a href="#"><i class="fa fa-dashboard"></i> Level</a></li>
                  <li class="active">Here</li>
                </ol>
            {% endblock %} 
          </section>

          <!-- Main content -->
          <section class="content">

              {% block content %}{% endblock %} 
            <!-- Your Page Content Here -->

          </section><!-- /.content -->
        </div><!-- /.content-wrapper -->

        <!-- Main Footer -->
        <footer class="main-footer">
          <!-- To the right -->
          <div class="pull-right hidden-xs">
            Version 0.1.0 (Alpha) 
          </div>
          <!-- Default to the left -->
          <strong>Copyright &copy; 2015 <a href="#">Paul Schweppe</a>.</strong> All rights reserved.
        </footer>

        <!-- Control Sidebar -->
        <aside class="control-sidebar control-sidebar-dark">
          <!-- Create the tabs -->
          <ul class="nav nav-tabs nav-justified control-sidebar-tabs">
            <li class="active"><a href="#control-sidebar-issue-tab" data-toggle="tab"><i class="fa fa-home"></i></a></li>
            <li><a href="#control-sidebar-settings-tab" data-toggle="tab"><i class="fa fa-gears"></i></a></li>
          </ul>
          <!-- Tab panes -->
          <div class="tab-content">
            <!-- Issue tab content -->
            {% if(project is defined) %}
            <div class="tab-pane active" id="control-sidebar-issue-tab" data-loaded="false" data-url="{{ path('issues_latest', { 'id': project.id }) }}">
                
              <h3 class="control-sidebar-heading">Loading...</h3>
              
            </div><!-- /.tab-pane -->
            {% else %}
                <div class="tab-pane active" id="control-sidebar-issue-tab" data-loaded="true" >
                
                    <h3 class="control-sidebar-heading">Error: Project ID not set</h3>
              
                </div><!-- /.tab-pane -->
            {% endif %}
            <!-- Stats tab content -->
            <div class="tab-pane" id="control-sidebar-stats-tab">Stats Tab Content</div><!-- /.tab-pane -->
            <!-- Settings tab content -->
            <div class="tab-pane" id="control-sidebar-settings-tab">
                <h3 class="control-sidebar-heading">Comming Soon</h3>
            </div><!-- /.tab-pane -->
          </div>
        </aside><!-- /.control-sidebar -->
        <!-- Add the sidebar's background. This div must be placed
             immediately after the control sidebar -->
        <div class="control-sidebar-bg"></div>
      </div><!-- ./wrapper -->

      <!-- REQUIRED JS SCRIPTS -->
      
      {% block footer %}
             <!-- jQuery -->
            <script src="{{ asset('bundles/versioncontrolgitcontrol/js/jquery.js') }}"></script>

            <!-- Bootstrap Core JavaScript -->
            <script src="{{ asset('bundles/versioncontrolgitcontrol/bootstrap/js/bootstrap.min.js') }}"></script>
            
            <script src="{{ asset('bundles/versioncontrolgitcontrol/theme/js/app.js') }}"></script>
            
             <!-- tiny color js-->
            <script src="{{ asset('bundles/versioncontrolgitcontrol/js/tinycolor-min.js') }}"></script>
            
             <!-- pick a color js -->
            <script src="{{ asset('bundles/versioncontrolgitcontrol/js/pick-a-color-1.2.3.min.js') }}"></script>
            
             <!-- Applications javascript for the full site-->
            <script src="{{ asset('bundles/versioncontrolgitcontrol/js/select2/v3.5/select2.min.js') }}"></script>
            
            <script src="{{ asset('bundles/versioncontrolgitcontrol/theme/plugins/prism/prism.js') }}"></script>
   
            <script src="{{ asset('bundles/versioncontrolgitcontrol/theme/plugins/pace/pace.min.js') }}"></script>
            
            <script src="{{ asset('bundles/versioncontrolgitcontrol/theme/plugins/loadmask/jquery.loadmask.spin.js') }}"></script>
            
            <script src="{{ asset('bundles/versioncontrolgitcontrol/theme/plugins/jquery-ui/jquery-ui.min.js') }}"></script>
            <script src="{{ asset('bundles/versioncontrolgitcontrol/theme/plugins/jquery-ui/jquery.ui.touch-punch.min.js') }}"></script>
            
            <script src="{{ asset('bundles/versioncontrolgitcontrol/js/modal.js') }}"></script>
            
            <script src="{{ asset('bundles/versioncontrolgitcontrol/js/app.js') }}"></script>
        {% endblock %} 
            
        {% block footerJS %}
        {% endblock %} 

    {% endblock %} 
    
    <!-- Default bootstrap modal example -->
    <div class="modal modal-info fade" id="issueModal" tabindex="-1" role="dialog" aria-labelledby="issueModalLabel" aria-hidden="true">
      <div class="modal-dialog">
        <div class="modal-content">
          <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            <h4 class="modal-title" id="issueModalLabel">Loading Data</h4>
          </div>
          <div class="modal-body">
            
          </div>
          <div class="modal-footer">
            <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
          </div>
        </div>
      </div>
    </div>
  </body>
</html>